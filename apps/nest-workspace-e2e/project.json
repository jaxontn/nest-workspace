{
  "name": "nest-workspace-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["nest-workspace"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "apps/nest-workspace-e2e/jest.config.cts",
        "passWithNoTests": true
      },
      "dependsOn": ["nest-workspace:build", "nest-workspace:serve"]
    }
  }
}
